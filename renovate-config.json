{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "group:allNonMajor",
    "schedule:nonOfficeHours",
    ":automergeMinor",
    ":gitSignOff",
    ":preserveSemverRanges",
    ":semanticCommits",
    ":semanticCommitTypeAll(chore)"
  ],
  "labels": ["dependencies"],
  "automergeStrategy": "squash",
  "dependencyDashboard": false
}
